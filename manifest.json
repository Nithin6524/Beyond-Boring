{
  "manifest_version": 3,
  "name": "Beyond Boring",
  "version": "1.0",
  "description": "Beyond Boring reimagines your Google homepage, infusing it with creativity and excitement for a refreshing online experience",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/infinity-16.png",
    "32": "images/infinity-32.png",
    "48": "images/infinity-48.png",
    "128": "images/infinity-128.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "topSites",
    "sidePanel",
    "storage",
    "tabs"
  ]
}